<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.AccountEntity">
        <table name="account" schema="quizappdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="uid">
                <column name="uid" nullable="false" precision="-1"/>
            </basic>
            <basic name="acount">
                <column name="acount" nullable="false" length="50"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false" length="100"/>
            </basic>
            <basic name="active">
                <column name="active" nullable="false" precision="1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
