<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.QuestionEntity">
        <table name="question" schema="quizappdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="bid">
                <column name="bid" nullable="false" precision="-1"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false" precision="-1"/>
            </basic>
            <basic name="content">
                <column name="content" nullable="false" length="1000"/>
            </basic>
            <basic name="answer">
                <column name="answer" nullable="false" length="1000"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
