<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.QuiztestEntity">
        <table name="quiztest" schema="quizappdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="50"/>
            </basic>
            <basic name="point">
                <column name="point" nullable="false" precision="-1"/>
            </basic>
            <basic name="time">
                <column name="time" nullable="false" precision="-1"/>
            </basic>
            <basic name="start">
                <column name="start" nullable="false"/>
            </basic>
            <basic name="end">
                <column name="end" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
